<%= simple_form_for(@swot) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :company_name %>
    <%= f.input :strength %>
    <%= f.input :weak %>
    <%= f.input :opportunity %>
    <%= f.input :threat %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
