<h1>Listing swots</h1>

<table>
  <thead>
    <tr>
      <th>Company name</th>
      <th>Strength</th>
      <th>Weak</th>
      <th>Opportunity</th>
      <th>Threat</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @swots.each do |swot| %>
      <tr>
        <td><%= swot.company_name %></td>
        <td><%= swot.strength %></td>
        <td><%= swot.weak %></td>
        <td><%= swot.opportunity %></td>
        <td><%= swot.threat %></td>
        <td><%= link_to 'Show', swot %></td>
        <td><%= link_to 'Edit', edit_swot_path(swot) %></td>
        <td><%= link_to 'Destroy', swot, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Swot', new_swot_path %>
